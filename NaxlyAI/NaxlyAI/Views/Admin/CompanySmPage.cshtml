@model NaxlyAI.Models.SocialMediaOfCompany
@{
    ViewData["Title"] = ViewData["PageDesc"];
}

<h3>@ViewData["PageDesc"]</h3>
<form method="POST">
    <div class="form-group">
        <label asp-for="Name">Name of Social Media of Company</label>
        <input asp-for="Name" class="form-control form-control-lg" />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Icon">Icon of Social Media of Company</label>
        <input asp-for="Icon" class="form-control form-control-lg" />
        <span asp-validation-for="Icon" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Url">Url of Social Media of Company</label>
        <input asp-for="Url" class="form-control form-control-lg" />
        <span asp-validation-for="Url" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary mb-2">@ViewData["Button"]</button>
</form>